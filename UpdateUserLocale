//

String LOCALE_SID_US = 'en_US';
String LOCALE_SID_CA = 'en_CA';
String QUERY_LOCALE_SID_CA = 'Select Id from User where LocaleSidKey = :LOCALE_SID_CA';

List<User> localeCAList = new List<User>();
List<User> localeUSList = new List<User>();

localeCAList = Database.Query(QUERY_LOCALE_SID_CA);

for(User u: localeCAList)
{
    User localeUSUser = new User();
    
    localeUSUser.Id = u.Id;
    localeUSUser.LocaleSidKey = LOCALE_SID_US;
    
    localeUSList.add(localeUSUser);
}

system.debug(localeUSList.size());
//update localeUSList;
