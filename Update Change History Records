List<BMCServiceDesk__Change_History__c> chList = new List<BMCServiceDesk__Change_History__c>();
List<BMCServiceDesk__Change_History__c> chUpdateList = new List<BMCServiceDesk__Change_History__c>();
chList = Database.Query('Select Id, Date_Time__c, CreatedDate from BMCServiceDesk__Change_History__c');

for(BMCServiceDesk__Change_History__c ch: chList)
{
    BMCServiceDesk__Change_History__c chRec = new BMCServiceDesk__Change_History__c();
    chRec.Id = ch.Id;
    chRec.Date_Time__c = ch.CreatedDate;
    
    chUpdateList.add(chRec);
}
update chUpdateList;
